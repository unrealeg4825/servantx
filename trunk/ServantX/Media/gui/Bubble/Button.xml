<?xml version="1.0" encoding="ANSI"?>
<AlbaLynx>
	<!-- =========================================== -->
	<!-- Default template for "Button" element -->
	<template Name="Button">
		<!-- Button background color -->
		<primitive Name="bg_color" Type="Texture_9" Margin="3">
			<color AllCorners="30 30 30 255" />
			<texture
				Image="BubbleSkin"
				Border="6"
				TexX_All="0		6	8	14"
				TexY_All="72	78	80	86"
			/>
		</primitive>
		<!-- State: Passive -->
		<primitive Name="BodyPassive" Type="Texture_9">
			<state Type="Partial" Positive="None" Negative="Locked | Active | Pressed | Checked" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="0		9		23		32"
			/>
		</primitive>
		<!-- State: Active -->
		<primitive Name="BodyActive" Type="Texture_9">
			<state Type="Partial" Positive="Active" Negative="Locked | Pressed | Checked" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="34	43		57		66"
			/>
		</primitive>
		<!-- State: Pressed -->
		<primitive Name="BodyPressed" Type="Texture_9">
			<state Type="Partial" Positive="Pressed" Negative="Locked | Checked" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="68	77		91		100"
			/>
		</primitive>
		<!-- State: Locked -->
		<primitive Name="BodyLock" Type="Texture_9">
			<state Type="Partial" Positive="Locked" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="102	111		125		134"
			/>
		</primitive>
		<!-- State: Checked -->
		<primitive Name="BodyChecked" Type="Texture_9">
			<state Type="Partial" Positive="Checked" Negative="Locked | Active | Pressed" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="136	145		159		168"
			/>
		</primitive>
		<!-- State: Checked & Active -->
		<primitive Name="BodyCheckedA" Type="Texture_9">
			<state Type="Partial" Positive="Checked | Active" Negative="Locked | Pressed" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="170	179		193		202"
			/>
		</primitive>
		<!-- State: Checked & Pressed -->
		<primitive Name="BodyCheckedP" Type="Texture_9">
			<state Type="Partial" Positive="Checked | Pressed" Negative="Locked" />
			<texture
				Image="BubbleSkin"
				Border="9"
				TexX_All="103	112		126		135"
				TexY_All="204	213		227		236"
			/>
		</primitive>
		<!-- Position of the icon. Horizontal collision with the text is enabled -->
		<primitive Name="Icon" Type="Icon" Width="16" Height="16" Margin="0 -3 0 10" Align="Center" Collision="1" />
		<!-- The text of the button. Horizontal collision with the icon is enabled -->
		<primitive Name="Caption" Type="Text" Margin="7 10 8" Width="-1" Height="-1" Align="Center" Collision="1">
			<color AllCorners="210 215 220 255" />
			<text Align="Center" />
		</primitive>
	</template>
	<!-- The end of "Button" template -->
	<!-- =========================================== -->

